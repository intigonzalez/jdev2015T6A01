<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:include schemaLocation="comment.xsd"></xs:include>
    <xs:element name="video">
		<xs:complexType>
			<xs:sequence>
                <xs:element name="name" type="xs:string" maxOccurs="1" minOccurs="1" />
                <xs:element name="id" type="xs:string" maxOccurs="1" minOccurs="0"></xs:element>
                <xs:element name="login" type="xs:string" maxOccurs="1"
					minOccurs="1" />

				<xs:element name="unix_time" type="xs:long"
					maxOccurs="1" minOccurs="1" />
				<xs:element name="link" type="xs:string" maxOccurs="1"
					minOccurs="1" />
				<xs:element ref="comment" maxOccurs="unbounded"
					minOccurs="0">
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>


</xs:schema>